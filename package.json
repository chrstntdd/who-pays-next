{
  "name": "who-pays-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "watch": "tsc && concurrently \"npm run tsc:w\" ",
    "hapi": "node server/server.js",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "html": "find ./client/scripts -name '*.html' -exec cp --parents \\{\\} ./dist \\;",
    "test": "./node_modules/tslint/bin/tslint main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcodonnell/who-pays-next.git"
  },
